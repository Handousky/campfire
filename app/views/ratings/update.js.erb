$('#star_<%= @story.id %>').raty({
  readOnly: true,
  score: <%= @story.avg_rating %>,
  path: '/assets'
});

$('#user_star_<%= @story.id %>').raty({
    score: <%= @rating.score %>,
    path: '/assets',
    click: function(score, evt) {
      $.ajax({
        url: '/ratings/' + <%= @rating.id %>,
        type: 'PATCH',
        data: { score: score }
      }).done(function (data){});
    }
  });
